-- Generate a column from 0 to 99
select (one + ten) as num from 
(select 0 one
union all
select 1 one
union all 
select 2 one
union all 
select 3 one
union all 
select 4 one
union all 
select 5 one
union all 
select 6 one
union all 
select 7 one
union all 
select 8 one
union all 
select 9 one) ones, 
(select 0 ten
union all
select 10 ten
union all 
select  20 ten
union all 
select 30 ten
union all 
select 40 ten
union all 
select 50 ten
union all 
select 60 ten
union all 
select 70 ten
union all 
select 80 ten
union all 
select 90 ten) tens; 
